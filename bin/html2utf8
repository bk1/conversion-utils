#!/usr/bin/perl

# usage

# html2utf8 < input > output

# converts a file from HTML-entities to UTF-8

use strict;
use utf8;
use Encode qw(decode encode);

my $outmode = "utf8";
my $inmode  = "utf8";
my $dinmode = "UTF-8";
my $doutmode = "UTF-8";

sub usage($) {
    my $msg = $_[0];
    if ($msg) {
        chomp $msg;
        print $msg, "\n\n";
    }
    print <<"EOUSAGE";
USAGE

$0 < inputfile > outputfile

converts HTML-entities from inputfile into Unicode characters and writes the result into outputfile

$0 inputfile1 inputfile2 inputfile3 ... inputfilen > outputfile

converts HTML-entities from concatenated inputfiles into Unicode characters and writes the result into outputfile

html2utf8 ... > outputfile

encodes the output in UTF-8

html2utf16 ... > outputfile

encodes the output in UTF-16
EOUSAGE
    if ($msg) {
        exit(1);
    } else {
        exit(0);
    }
}

for my $arg (@ARGV) {
    if ($arg =~m/--?help|-H|-\?/) {
        usage(0);
    }
}

if ($0 =~ m/html2utf16$/) {
    $outmode = 'UTF-16';
    $doutmode = 'UTF-16';
}

print STDERR "Input Encoding: $dinmode; Output Encoding: $doutmode\n";
# use open IN => $inmode;
# use encoding $inmode, STDOUT => $outmode;

use open IN => ":utf8";
binmode STDIN, ":utf8";
binmode STDOUT, ":$outmode";

