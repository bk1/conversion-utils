while (<>) {
    my $orig = $_;
    s/_/_lowbar;/g;
    s/&(lowbar|UnderBar|#x0*5F|#0*95);/_lowbar;/g;
    s/&(QUOT|quot|#x0*22|#0*34);/_quot;/g;
    s/&(AMP|amp|#x0*26|#0*38);/_amp;/g;
    s/&(LT|lt|#x0*3[cC]|#0*60);/_lt;/g;
    s/&(GT|gt|#x0*3[eE]|#0*62);/_gt;/g;
    s/&(apos|#x0*27|#0*39);/_apos;/g;
    while (m/\&(?<ENT>[A-Za-z0-9#]+);/) {
        my $ent = $+{ENT};
        my $c = $map{$ent};
        unless (defined $c) {
            if ($ent =~ m/^#x(?<HEX>[0-9A-Fa-f]+)$/) {
                my $n = hex($+{HEX});
                $c = chr($n);
                $map{$ent} = $c;
            } elsif ($ent =~ m/^#(?<DEC>\d+)$/) {
                $c = chr($+{DEC});
                $map{$ent} = $c;
            } else {
                $c = "_$ent;";
                $map{$ent} = $c;
                chomp $orig;
                print STDERR "line $.: illegal entity &$ent; in \"$orig\"\n";
            }
        }
        s/\&$ent;/$c/g;
    }
    s/_([a-zA-Z0-9#]+);/&$1;/g;
    s/\&lowbar;/_/g;
    print encode($outmode, $_);
}
